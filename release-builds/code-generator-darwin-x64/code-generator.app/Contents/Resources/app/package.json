{
  "name": "code-generator",
  "version": "1.0.0",
  "description": "A Simple Code Generator (Currently: C++)",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . code-generator --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Compiler Project\"",
    "package-linux": "electron-packager . code-generator --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/512x512.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrWooJ/Code-Generator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrWooJ/Code-Generator/issues"
  },
  "homepage": "https://github.com/MrWooJ/Code-Generator#readme",
  "dependencies": {
    "adminbsb-materialdesign": "^1.0.5",
    "bootstrap": "^3.3.7",
    "electron": "^1.8.4",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^12.0.1"
  }
}
